@model API.Operaciones.ComplementoCartaPorte.ContenedorM


@using (Html.BeginCollectionItem("ContenedoresM"))
{
    @Html.ValidationSummary(true, "", new { @class = "label-blue-alt" })

    <div class="card shadow-sm">
        <div class="card-header">
            <h3 class="card-title">
                Contenedor Maritimo
            </h3>
            <div class="card-toolbar">
                <a href="#" class="icon-separator" onclick="removeDetalleTMContenedor(this)">
                    <!--begin::Svg Icon -->
                    <span class="svg-icon svg-icon-3x">
                        <img src="~/_CDN/assets/media/icons/duotune/general/gen034.svg" width="30" height="30" />
                    </span>
                    <!--end::Svg Icon-->
                </a>
            </div>
        </div>
        <div class="card-body row TMContenedorEdit">
            @Html.HiddenFor(model => model.Id)
            @Html.HiddenFor(model => model.TransporteMaritimo_Id)
            <div class="form-group col-xs-12 col-sm-3">
                @Html.LabelFor(model => model.ContenedorMaritimo_Id)<br />
                @Html.DisplayFor(model => model.ContenedorMaritimo_Id)
                @Html.HiddenFor(model => model.ContenedorMaritimo_Id)
            </div>
            <div class="form-group col-xs-12 col-sm-3">
                @Html.LabelFor(model => model.MatriculaContenedor)<br />
                @Html.DisplayFor(model => model.MatriculaContenedor)
                @Html.HiddenFor(model => model.MatriculaContenedor)
            </div>
            <div class="form-group col-xs-12 col-sm-3">
                @Html.LabelFor(model => model.NumPrecinto)<br />
                @Html.DisplayFor(model => model.NumPrecinto)
                @Html.HiddenFor(model => model.NumPrecinto)
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <div class="clearfix"></div>
    <br />
}

