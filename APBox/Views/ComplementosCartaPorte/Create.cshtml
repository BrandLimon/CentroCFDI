<script>
    $('.cfdiRelacionado').change(function () {
        if ($('.cfdiRelacionado').prop('checked')) {
            $('.cfdiRelacionadoSeccion').show(200);
            $('.cfdiRelacionadoSeccion').css('display', 'inline');
        } else {
            $('.cfdiRelacionadoSeccion').hide(200);
        }
    });

    $('#agregarDetalleUbicacion').click(function () {
        var contador = $(".Ubicaciones").size() + 1;
        $(".contadorUbicaciones").replaceWith('<span class="contadorPagos font-blue">: ' + contador + '</span >');
    });

    $("#agregarDetalleUbicacion").on('click', function () {
        var ORNE = "";
        var DENE = "";
        if ($("#DiveEstacionMaritima").is(":visible")) {
            ORNE = $('#NombreEstacionMaritima option:selected').text();
        }
        if ($("#DivEstacionAereo").is(":visible")) {
            ORNE = $('#NombreEstacionAereo option:selected').text();
        }
        if ($("#DivEstacionFerroviaria").is(":visible")) {
            ORNE = $('#NombreEstacionFerroviaria option:selected').text();
        }
        if ($("#DivEstacionMaritimaD").is(":visible")) {
            DENE = $('#NombreEstacionMaritimaD option:selected').text();
        }
        if ($("#DivNombreEstacionAereoD").is(":visible")) {
            DENE = $('#NombreEstacionAereoD option:selected').text();
        }
        if ($("#DivNombreEstacionFerroviariaD").is(":visible")) {
            DENE = $('#NombreEstacionFerroviariaD option:selected').text();
        }
            $.ajax({
                type: 'POST',
                url: ' @Url.Action("AgregarUbicacion", "AjaxOperaciones")',
                data: JSON.stringify({
                    DistanciaRecorrida: $('#Ubicacion_DistanciaRecorrida').val(),
                    TipoEstacionId: $('#Ubicacion_TipoEstacion option:selected').val(),
                    TipoEstacion: $('#Ubicacion_TipoEstacion option:selected').text(),
                    IdOrigen: $('#Ubicacion_UbicacionOrigen_IdOrigen').val(),
                    ORRFCRemitente: $('#Ubicacion_UbicacionOrigen_RFCRemitente').val(),
                    ORNombreRemitente: $('#Ubicacion_UbicacionOrigen_NombreRemitente').val(),
                    ORResidenciaFiscal: $('#Ubicacion_UbicacionOrigen_ResidenciaFiscal').val(),
                    ORNombreEstacion: ORNE,
                    ORNumEstacion: $('#Ubicacion_UbicacionOrigen_NumEstacion').val(),
                    FechaHoraSalida: $('#Ubicacion_UbicacionOrigen_FechaHoraSalida').val(),
                    ORCalle: $('#Ubicacion_UbicacionOrigen_Domicilio_Calle').val(),
                    ORCodigoPostal: $('#Ubicacion_UbicacionOrigen_Domicilio_CodigoPostal').val(),
                    ORColonia: $('#Ubicacion_UbicacionOrigen_Domicilio_Colonia').val(),
                    OREstado: $('#Ubicacion_UbicacionOrigen_Domicilio_Estado').val(),
                    ORLocalidad: $('#Ubicacion_UbicacionOrigen_Domicilio_Localidad').val(),
                    ORMunicipio: $('#Ubicacion_UbicacionOrigen_Domicilio_Municipio').val(),
                    ORNumeroExterior: $('#Ubicacion_UbicacionOrigen_Domicilio_NumeroExterior').val(),
                    ORNumeroInterior: $('#Ubicacion_UbicacionOrigen_Domicilio_NumeroInterior').val(),
